    <!-- jQuery -->
    <script src="~/Assets/admin/vendor/jquery/jquery.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Assets/admin/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/Assets/admin/vendor/metisMenu/metisMenu.min.js"></script>
    <script src="~/Assets/admin/plugins/ckfinder/ckfinder.js"></script>
    <script src="~/Assets/admin/plugins/ckeditor/ckeditor.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="~/Assets/admin/dist/js/sb-admin-2.js"></script>
    <script>
        function LeftMenuFunction() {
            var x = document.getElementById("myLeftMenu");
            var y = document.getElementById("myBodyPage");
            if (x.style.display === "none") {
                x.style.display = "block";
                y.style.marginLeft = "0px";
            } else {
                x.style.display = "none";
                y.style.marginLeft = "-250px";
            }
        }
    </script>

    @RenderSection("jsPluginCK", required: false)
