            </div>
            <div class="clearfix"> </div>
            <!--Phân trang-->
            @if (ViewBag.TotalPage > 1)
            {
                var pageDisplay = (int)ViewBag.MaxPage;
                int totalPage = (int)ViewBag.TotalPage;
                int currentPage = ((int)ViewBag.Page);

                // Create numeric links
                var startPageIndex = Math.Max(1, currentPage - pageDisplay / 3);
                var endPageIndex = Math.Min(totalPage, currentPage + pageDisplay / 3);

                var link = "/sanpham/" + category.MetaTitle + "-" + category.CategoryID; ;

                <nav>
                    <ul class="pagination">
                        @if (currentPage > ViewBag.First)
                        {
                            <li><a href="@link?page=@ViewBag.First" aria-label="First" title="Trang đầu"><span aria-hidden="true">«</span></a></li>
                            @*<li><a href="@link?page=@ViewBag.Prev" aria-label="Previous" title="Trang trước"><span aria-hidden="true">»</span></a></li>*@
                        }
                        @for (int i = startPageIndex; i <= endPageIndex; i++)
                        {
                            if (currentPage == i)
                            {

                                <li> <a href="@link?page=@i" class="active" title="Trang @i">@i</a></li>
                            }
                            else
                            {
                                <li><a href="@link?page=@i" title="Trang @i" class="pos">@i</a></li>
                            }
                        }
                        @if (currentPage < totalPage)
                        {
                            @*<li><a href="@link?page=@ViewBag.Next" aria-label="Next" title="Trang sau"><span aria-hidden="true">«</span></a></li>*@
                            <li><a href="@link?page=@ViewBag.Last" aria-label="Last" title="Trang cuối"><span aria-hidden="true">»</span></a></li>
                        }
                    </ul>
                </nav>
            }
        </div>
    </div>
</div>
