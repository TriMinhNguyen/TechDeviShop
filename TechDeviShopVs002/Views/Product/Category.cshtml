                                    <p><span>@(item.Price.HasValue ? item.Price.Value.ToString("N0") : "Liên hệ")</span> <i class="item_price">@(item.PromotionPrice.HasValue ? item.PromotionPrice.Value.ToString("N0") : "Liên hệ")</i></p>
                                    <form action="#" method="post">
                                        @*<input type="hidden" name="cmd" value="_cart" />
                                        <input type="hidden" name="add" value="1" />
                                        <input type="hidden" name="w3ls_item" value="@item.ProductName" />
                                        <input type="hidden" name="amount" value="@item.PromotionPrice" />
                                        <button type="submit" class="w3ls-cart">Thêm vào giỏ hàng</button>*@
                                        <a class="btn btn-default" href="/them-gio-hang?productId=@item.ProductID&quantity=1">Thêm vào giỏ hàng</a>
                                    </form>
